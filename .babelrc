{
  "plugins": [
    ["transform-async-to-module-method", {
      "module": "bluebird",
      "method": "coroutine"
    }]
  ],
  "env": {
    "development": {
      "presets": [
        ["env", {
          "targets": {
            "node": "6.10"
          }
        }]
      ]
    },
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": "4.0"
          }
        }]
      ]
    },
    "production": {
      "presets": [
        ["env", {
          "targets": {
            "node": "4.0"
          }
        }]
      ],
      "plugins": [
        "minify-mangle-names"
      ]
    }
  }
}