{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env", {
            "targets": {
              "node": "6"
            }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env", {
            "targets": {
              "node": "6"
            }
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env", {
            "targets": {
              "node": "6"
            }
          }
        ]
      ]
    }
  },
  "plugins": [
    [
      "transform-async-to-module-method", {
        "module": "bluebird",
        "method": "coroutine"
      }
    ]
  ]
}